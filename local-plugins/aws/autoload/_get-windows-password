#compdef get-windows-password

local -a values
values=()

_arguments '1:instance-id:' '2::ssh-keypair: _alternative
    "ssh-keypair:ssh keypair from .ssh:_path_files \
        -W $HOME/.ssh \
        -F config \
        -F authorized_keys \
        -F known_hosts \
        -F \"*.pub\" \
        -P $HOME/.ssh/"
    "ssh-keypair2:ssh keypair:_files"
'
